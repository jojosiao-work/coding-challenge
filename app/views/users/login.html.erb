<div class="headline">Login</div>

<%= form_with(url: "/users/login") do |f| %>

  
    <div class="form-group">
      <%= f.label :email %>
      <%= f.text_field :email %>
    </div>
  
    <div class="form-group">
    <%= f.label :password %>
    <%= f.password_field :password  %>
    </div>
    
    <%= f.submit "Login" %>
    <div>
      <%= link_to  "Register" ,"/users/register" %>
    </div>

<% end %>
  
  